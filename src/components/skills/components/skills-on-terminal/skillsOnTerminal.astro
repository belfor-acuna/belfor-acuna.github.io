---
import TerminalIcon from '../../atoms/terminal-icon/TerminalIcon.astro';
import Terminal from '../terminal/Terminal.astro';
import './skills-on-terminal.css';
---

<Terminal nombre="b.acuna">
	<div class="terminal__skills">
		<h2>My skills</h2>
		<div class="terminal__skills__container">
			<TerminalIcon
				src="/assets/javascript.svg"
				color="#7738ff"
				name="Javascript"
			/>
			<TerminalIcon
				src="/assets/typescript.svg"
				color="#00D1FF"
				name="Typescript"
			/>
			<TerminalIcon
				src="/assets/typescript.svg"
				color="#00D1FF"
				name="Java"
			/>
			<TerminalIcon
				src="/assets/angular.svg"
				color="#FF4747"
				name="NestJS"
			/><TerminalIcon
			src="/assets/vue.svg"
			color="#00D1FF"
			name="VueJS"
			/><TerminalIcon
			src="/assets/react.svg"
			color="#00D1FF"
			name="Springboot"
			/>
		</div>
	</div>
</Terminal>
